{
  "manifest_version": 2,
  "name": "browser-source-provider",
  "version": "1.0.0",
  "description": "chrome extension for coc-browser",
  "author": "voldikss",
  "browser_action": {
    "default_title": "browser-source-provider"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "https://127.0.0.1/*",
    "http://127.0.0.1/*"
  ]
}
