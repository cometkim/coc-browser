{
  "name": "coc-browser",
  "version": "1.2.6",
  "description": "browser words completion for coc.nvim",
  "main": "lib/index.js",
  "engines": {
    "coc": "^0.0.74"
  },
  "keywords": [
    "coc.nvim",
    "completion",
    "neovim",
    "vim"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build",
    "test": "jest --forceExit"
  },
  "activationEvents": [
    "*"
  ],
  "repository": "https://github.com/voldikss/coc-browser",
  "homepage": "https://github.com/voldikss/coc-browser/#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "browser.enable": {
          "type": "boolean",
          "default": true,
          "description": "whether to enable coc-browser"
        },
        "browser.shortcut": {
          "type": "string",
          "default": "WEB"
        },
        "browser.capacity": {
          "type": "number",
          "default": 5,
          "description": "count of file cache which is used to storage words, 1~10"
        },
        "browser.priority": {
          "type": "number",
          "default": 5
        },
        "browser.port": {
          "type": "number",
          "default": 8888,
          "description": "port used to communication with browser extension"
        },
        "browser.minLength": {
          "type": "number",
          "default": 4,
          "description": "Dismiss these words whose length is smaller than this option's value"
        },
        "browser.maxLength": {
          "type": "number",
          "default": 20,
          "description": "Dismiss these words whose length is larger than this option's value"
        }
      }
    },
    "commands": [
      {
        "title":"Clear browser completion source cache",
        "command": "clearCandidates"
      }
    ]
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@voldikss/tsconfig": "*",
    "@voldikss/tslint-config": "*",
    "@types/node": "^13.7.7",
    "coc.nvim": "^0.0.75",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}
