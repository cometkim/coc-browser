{
  "name": "coc-browser",
  "version": "1.1.0",
  "description": "browser word completion for coc.nvim",
  "main": "lib/index.js",
  "engines": {
    "coc": "^0.0.72"
  },
  "keywords": [
    "coc.nvim",
    "completion",
    "neovim",
    "vim"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build",
    "test": "jest --forceExit"
  },
  "activationEvents": [
    "*"
  ],
  "repository": "https://github.com/voldikss/coc-browser",
  "homepage": "https://github.com/voldikss/coc-browser/#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.browser.enable": {
          "type": "boolean",
          "default": true,
          "description": "whether to enable coc-browser"
        },
        "coc.browser.shortcut": {
          "type": "string",
          "default": "WEB"
        },
        "coc.browser.capacity": {
          "type": "number",
          "default": 5,
          "description": "count of file cache which is used to storage words, 1~10"
        },
        "coc.browser.priority": {
          "type": "number",
          "default": 5
        }
      }
    }
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.18",
    "@types/node": "^12.0.10",
    "coc.nvim": "^0.0.73",
    "request-light": "^0.2.4",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {}
}
